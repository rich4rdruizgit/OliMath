<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="olimpiadas.sena.com.olimpiadasmath">

    <application
        android:name=".control.MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/icono"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icono"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.menu.MainActivity" />
        <activity android:name=".activities.test.TestActivity" />
        <activity android:name=".activities.shop.ShopActivity" />
        <activity android:name=".activities.ranking.RankingActivity" />
        <activity android:name=".activities.statistics.StatisticActivity" />
        <activity android:name=".activities.profile.ProfileActivity" />
        <activity android:name=".activities.splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.settings.SettingsActivity" />
        <activity android:name=".activities.result.ResultActivity"></activity>
    </application>

</manifest>